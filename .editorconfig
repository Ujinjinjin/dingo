root = true

[*]
# Unix-style newlines with a newline ending every file
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
# Set default charset
charset = utf-8

# tab indentation
[*.cs]
indent_style = tab
indent_size = 4

# tab indentation
[Makefile]
indent_style = tab
indent_size = 4
